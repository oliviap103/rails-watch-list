    <div class="banner">
      <div class="banner-container">
        <h1>Movie Lists</h1>
        <%= link_to "Add List", new_list_path, class:'btn-green'%>
      </div>
    </div>
    <div class="card-container">
      <div class="cards">
        <% @lists.each do |list| %>
          <%= link_to list_path(list), class:'btn-list text-decoration-none' do%>
            <div class="card">
              <img src="https://w7.pngwing.com/pngs/545/554/png-transparent-computer-icons-film-movie-projector-action-movie-angle-text-rectangle.png"/>
              <h2><%= list.name %></h2>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
